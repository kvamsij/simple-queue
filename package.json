{
  "name": "@kvamse/simple-queue",
  "version": "1.0.2",
  "description": "A lightweight TypeScript queue system with persistence, priority support, and batch processing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "simple-queue": "dist/cli.js"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "watch": "nodemon --watch 'src/**/*.ts' --exec ts-node src/cli.ts",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "queue",
    "typescript",
    "async",
    "batch",
    "priority-queue",
    "persistence"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "Vamsi <kvamsij@gmail.com> (https://github.com/kvamsij)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kvamsij/simple-queue.git"
  },
  "bugs": {
    "url": "https://github.com/kvamsij/simple-queue/issues"
  },
  "homepage": "https://github.com/kvamsij/simple-queue#readme",
  "license": "MIT",
  "dependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.3.1"
  }
}
